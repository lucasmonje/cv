function viewLoad(){actualizarEncabezado("<h4><strong>Valid√°<\/strong> tus datos<\/h4><h1>Para validar tu identidad, responde las siguientes preguntas de seguridad.<\/h1>","#opcValidacionIdentidad","headline small",!0);$(headerTime).insertAfter($("#shared_Headerheadline"));$("#CantPregContestadas").html($(".done").length+"/"+lengthPreguntas);$(".pregunta").each(function(n){$(this).children().click(function(){AgregarValorHidden($(this),"SpanRespSelec-"+n,"Idresp-"+n);$("#DivPreg-"+n).attr("class","questions done");$("#CantPregContestadas").html($(".done").length+"/"+lengthPreguntas);$("#DivPreg-"+(n+1)).attr("class","questions selected");$("#Idresp-"+n).attr("class","Hidden");window.setTimeout(validarCamposRN,100)})});$("#btn-continuar").click(function(){stopDoubleSubmit("btn-continuar");$(".done").size()===lengthPreguntas&&validarPregAjax();$("#btn-continuar").prop("onclick",null).off("click")});intervalo=setInterval(CrearContador,1e3)}function CrearContador(){var n,t,r,i;tiempoSesion--;n=tiempoSesion;n<0&&flag===!1?(flag=!0,clearInterval(intervalo),showErrorModal("Mensaje","Finaliz&oacute; el tiempo disponible. Para solicitar un producto Galicia, por favor, acercarte a cualquiera de nuestras sucursales."),$("#btn_ModalGenerico").click(function(){window.location.href="/IngresoDatos/Inicio/"})):(t="",r=Math.floor(n/60),n=n%60,i=Math.floor(n),t+=r+":",t+=i.toString().length>1?i:"0"+i,$("#countbox1").html(t))}function validarCamposRN(){if($(".incompleta").length===0){$("#btn-continuar").attr("class","btn btn-xs margin-bottom-20");$("#btn-continuar").prop("disabled","");clearInterval(intervalo);return}return}function validarPregAjax(){var n="",t="";$(".Hidden").each(function(i){n+=$(this).val()+"-";t+=$("#IdPreg-"+i).val()+"-"});n=n.substring(0,n.length-1);t=t.substring(0,t.length-1);$.ajax({datatype:"json",type:"POST",url:"/ValidacionIdentidad/ValidarPreguntas",data:{IdResp:n,IdPreguntas:t},success:function(n){IsAjaxError(n)||(n.SegundaOportunidad?(showErrorModal("Mensaje","Para poder continuar verific&aacute; las respuestas ingresadas."),$("#btn_ModalGenerico").text("Reintentar"),$("#btn_ModalGenerico").click(function(){window.location.href="/ValidacionIdentidad/Inicio/"})):$("#FormRiesgoNet").submit())}})}function AgregarValorHidden(n,t,i){$("#"+t).text(n.html());$("#"+i).val(n.data("value"));$("#"+i).blur()}var flag=!1,intervalo,headerTime='<div class="row"><div class="col-xs-12 col-sm-12 col-md-12 col-lg-12"><div class="container"><div class="time-left"><p>Tiempo restante <strong id="countbox1"> 03:00<\/strong><\/p><div id="CantPregContestadas"><\/div><\/div><\/div><\/div><\/div>'